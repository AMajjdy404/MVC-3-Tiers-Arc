@model DepartmentViewModel

<div class="form-group">
    <label asp-for="Code"></label>
    <input readonly asp-for="Code" class="form-control" />

</div>
<div class="form-group">
    <label asp-for="Name"></label>
    <input readonly asp-for="Name" class="form-control" />
